{
  "name": "prkskrs-test-backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run clean-dist && npm run build-ts",
    "build-run": "npm run clean-dist && npm run build-ts  && npm run serve",
    "build-ts": "npx tsc",
    "clean-dist": "npx ts-node removeDist.ts",
    "lint": "eslint src --ext ts",
    "serve": "nodemon src/server.ts",
    "serve:dev": "npx ts-node src/server.ts",
    "heroku-postbuild": "npm run build",
    "build-test": "npm run kill-node && nodemon",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/server.js\"",
    "kill-node": "kill -9 `sudo lsof -t -i:4001`",
    "format": "npx prettier  'src/**/*.ts' --write",
    "watch-run": "concurrently --kill-others --names \"BUILD,RUN\" -c \"bgBlue.bold,bgMagenta.bold\" \"tsc -w\" \"nodemon -q -w dist dist/src/server.js\"",
    "test": "jest  --verbose --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.1",
    "@types/shelljs": "^0.8.12",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.54.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^6.12.0",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "eslint-config-prettier": "^9.0.0",
    "express": "^4.18.2",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.5",
    "typedi": "^0.8.0",
    "validator": "^13.9.0"
  }
}
